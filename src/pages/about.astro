---
import BackToPage from "../components/BackToPage.astro";
import BasePageLayout from "../layouts/BasePageLayout.astro";
import TopicListLayout from "../layouts/TopicListLayout.astro";
import { newIPageMock } from "../utils";

const title = "About";
---

<BasePageLayout title={title}>
  <TopicListLayout
    title={title}
    page={newIPageMock([
      {
        title: "Douglas Capeletti",
        url: "https://www.linkedin.com/in/douglas-capeletti/",
      },
    ])}
    blank
    wideCard
  />
  <BackToPage />
</BasePageLayout>
